{
  "name": "pinetype",
  "version": "1.0.0",
  "description": "Pinetype is an educational project to help all involved learn how to build an app from scratch, with no guidance or tutorials to follow.",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rosnovsky/pinetype.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/rosnovsky/pinetype/issues"
  },
  "homepage": "https://github.com/rosnovsky/pinetype#readme",
  "scripts": {
    "dev": "rollup -c -w",
    "start": "sirv public",
    "test": "jest --watchAll --coverage",
    "build": "rollup -c",
    "start:lambda": "netlify-lambda serve functions/mongodb-graphql/",
    "start:storybook": "start-storybook -p 6006",
    "build:lambda": "netlify-lambda build functions/mongodb-graphql/",
    "build:storybook": "build-storybook"
  },
  "dependencies": {
    "apollo-server-lambda": "^2.10.1",
    "encoding": "^0.1.12",
    "mongodb": "^3.5.3",
    "mongoose": "^5.9.1",
    "utf-8-validate": "^5.0.2",
    "@auth0/auth0-spa-js": "^1.6.4",
    "apollo-boost": "^0.4.7",
    "apollo-link": "^1.2.13",
    "apollo-link-http": "^1.5.16",
    "apollo-server-micro": "^2.10.1",
    "graphql": "^14.6.0",
    "sirv-cli": "^0.4.4",
    "svelte-apollo": "^0.3.0"
  },
  "devDependencies": {
    "netlify-lambda": "^1.4.5",
    "npm-run-all": "^4.1.5",
    "@babel/core": "^7.8.4",
    "@rollup/plugin-commonjs": "^11.0.0",
    "@rollup/plugin-node-resolve": "^7.0.0",
    "@storybook/addon-actions": "^5.3.12",
    "@storybook/addon-links": "^5.3.12",
    "@storybook/addons": "^5.3.12",
    "@storybook/svelte": "^5.3.12",
    "@testing-library/jest-dom": "^5.1.1",
    "@testing-library/svelte": "^1.11.0",
    "babel-loader": "^8.0.6",
    "jest": "^25.1.0",
    "jest-transform-svelte": "^2.1.1",
    "rollup": "^1.20.0",
    "rollup-plugin-livereload": "^1.0.0",
    "rollup-plugin-svelte": "^5.0.3",
    "rollup-plugin-terser": "^5.1.2",
    "svelte": "^3.18.1",
    "svelte-loader": "^2.13.6"
  },
  "jest": {
    "transform": {
      "^.+\\.js$": "babel-jest",
      "^.+\\.svelte$": "jest-transform-svelte"
    },
    "moduleFileExtensions": [
      "js",
      "svelte",
      "json"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|scss|stylesheet)$": "<rootDir>/__mocks__/styleMock.js"
    },
    "setupFilesAfterEnv": [
      "@testing-library/jest-dom/extend-expect"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/build/",
      "/storybook-static/"
    ]
  }
}
